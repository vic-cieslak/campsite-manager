#: kempingdabrowno/templates/account/password_change.html:7
#: kempingdabrowno/templates/account/password_change.html:10
#: kempingdabrowno/templates/account/password_change.html:16
#: kempingdabrowno/templates/account/password_reset_from_key.html:7
#: kempingdabrowno/templates/account/password_reset_from_key.html:14
#: kempingdabrowno/templates/account/password_reset_from_key_done.html:6
#: kempingdabrowno/templates/account/password_reset_from_key_done.html:9
msgid "Change Password"
msgstr "Zmień hasło"

#: kempingdabrowno/templates/account/password_reset.html:8
#: kempingdabrowno/templates/account/password_reset.html:11
#: kempingdabrowno/templates/account/password_reset_done.html:7
#: kempingdabrowno/templates/account/password_reset_done.html:10
msgid "Password Reset"
msgstr "Resetowanie hasła"

#: kempingdabrowno/templates/account/password_reset.html:16
msgid ""
"Forgotten your password? Enter your e-mail address below, and we'll send you "
"an e-mail allowing you to reset it."
msgstr ""
"Zapomniałeś hasła? Wprowadź poniżej swój adres e-mail, a my wyślemy Ci wiadomość e-mail umożliwiającą zresetowanie hasła."

#: kempingdabrowno/templates/account/password_reset.html:25
msgid "Reset My Password"
msgstr "Zresetuj moje hasło"

#: kempingdabrowno/templates/account/password_reset.html:27
msgid "Please contact us if you have any trouble resetting your password."
msgstr "Proszę skontaktuj się z nami, jeśli masz jakiekolwiek problemy z resetowaniem hasła."

#: kempingdabrowno/templates/account/password_reset_done.html:15
msgid ""
"We have sent you an e-mail. Please contact us if you do not receive it "
"within a few minutes."
msgstr ""
"Wysłaliśmy do Ciebie wiadomość e-mail. Proszę skontaktuj się z nami, jeśli nie otrzymasz jej w ciągu kilku minut."

#: kempingdabrowno/templates/account/password_reset_from_key.html:12
msgid "Bad Token"
msgstr "Nieprawidłowy token"

#: kempingdabrowno/templates/account/password_reset_from_key.html:20
#, python-format
msgid ""
"The password reset link was invalid, possibly because it has already been "
"used.  Please request a <a href=\"%(passwd_reset_url)s\">new password reset</"
"a>."
msgstr ""
"Link do resetowania hasła był nieprawidłowy, prawdopodobnie dlatego, że został już użyty. Proszę zażądać <a href=\"%(passwd_reset_url)s\">nowego resetowania hasła</a>."

#: kempingdabrowno/templates/account/password_reset_from_key.html:30
msgid "change password"
msgstr "zmień hasło"

#: kempingdabrowno/templates/account/password_reset_from_key_done.html:10
msgid "Your password is now changed."
msgstr "Twoje hasło zostało zmienione."

